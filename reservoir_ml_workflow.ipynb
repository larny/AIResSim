    # åå‘ä¼ æ’­
    loss.backward()
    optimizer.step()
    
    train_losses.append(loss.item())
    
    if (epoch + 1) % 5 == 0:
        print(f"Epoch {epoch+1}/{num_epochs}, Loss: {loss.item():.6f}")

print(f"\\nè®­ç»ƒå®Œæˆï¼Œæœ€ç»ˆæŸå¤±: {train_losses[-1]:.6f}")

